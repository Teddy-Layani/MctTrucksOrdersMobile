<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="saveDetailsDialog" showHeader="false" class="saveDialog" contentWidth="90%">
		<VBox alignItems="Center">
			<Image src="css/icons/success_Arrow.png" class="imgSuccessArrow"></Image>
			<Text text="{i18n>detailsSave}" class="detailSaveEl"/>
			<Button text="{i18n>backToActivity}" press="onCloseDialog"/>
		</VBox>
	</Dialog>
</core:FragmentDefinition>